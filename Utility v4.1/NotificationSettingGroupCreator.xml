<?xml version="1.0" encoding="utf-8"?>
<Form xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:dsp="http://www.gappex.com/sfp/DataSource/Parameters" Ident="NotificationSettingGroupCreator" SegmentType="Admin" PackageIdent="Utility">
	<DataPermissions>
		<string>SuperAdmin</string>
	</DataPermissions>
	<CreatePermissions>
		<string>SuperAdmin</string>
	</CreatePermissions>
	<Buttons>
		
		<Button xsi:type="FormButton" Ident="CreateButton" TitleResourceKey="CreateButton_Utility" IsSave="true" PlacementType="Top" ColorType="Success" IconCssClass="icon-floppy-disk" IsVisible="false" />
		<Button xsi:type="FormButton" Ident="UpdateButton" TitleResourceKey="UpdateButton_Utility" IsSave="true" PlacementType="Top" ColorType="Warning" IconCssClass="icon-floppy-disk" IsVisible="false" />
		<Button xsi:type="FormButton" Ident="DeleteButton" TitleResourceKey="DeleteButton_Utility" IsSave="false" ColorType="Danger" IsVisible="false" IconCssClass="icon-bin2">
			<Extensions>
				<Extension xsi:type="ConfirmDialogExtension" TitleResourceKey="DeleteDialogTitle_Utility" DescriptionResourceKey="DeleteDialogDescription_Utility" />
			</Extensions>
		</Button>
		<Button xsi:type="BackButton" Ident="BackButton" TitleResourceKey="BackButton_Utility" IconCssClass="icon-undo2" />

	</Buttons>
	<Controls>

		<Control xsi:type="AutoCompleteControl" Ident="OldID" MaxLength="256" DataType="String" TitleResourceKey="OldID_Utility" IsAnyValue="true">
      <DataBind DefaultTitleResourceKey="SelectValue" DefaultValue="">
        <Columns>
          <Column Ident="FolderGroupSegmentIdent" DataBindType="Value" />
          <Column Ident="TextValue" DataBindType="Title"/>
        </Columns>
        <SQL>
          SELECT ID, ID AS TextValue
					FROM dbo.NotificationSettingGroup
					WHERE ID like @OldID
        </SQL>
        <Parameters>
          <dsp:Parameter xsi:type="dsp:VariableParameter" Ident="OldID" DataType="String" LikeType="Both" MaxLenght="200" ></dsp:Parameter>
        </Parameters>
      </DataBind>
      <SelectedDataBind>
        <Columns>
          <Column Ident="FolderGroupSegmentIdent" DataBindType="Value" />
          <Column Ident="TextValue" DataBindType="Title"/>
        </Columns>
        <SQL>
          SELECT ID, ID AS TextValue
					FROM dbo.NotificationSettingGroup 
					WHERE ID = @OldID
        </SQL>
        <Parameters>
          <dsp:Parameter xsi:type="dsp:VariableParameter" Ident="OldID" DataType="String" ></dsp:Parameter>
        </Parameters>
      </SelectedDataBind>
    </Control>

		<Control xsi:type="TextBoxControl" Ident="NewID" MaxLength="256" DataType="String" TitleResourceKey="NewID_Utility">
			<DataBind>
				<Dependencies>
					<string>OldID</string>
				</Dependencies>
        <Columns>
          <Column Ident="NewID" DataBindType="Value" />
        </Columns>
        <SQL>
					SELECT @OldID AS NewID
        </SQL>
        <Parameters>
          <dsp:Parameter xsi:type="dsp:VariableParameter" Ident="OldID" DataType="String" />
        </Parameters>
      </DataBind>
		</Control>

		<Control xsi:type="TextBoxControl" Ident="ResourceKey" MaxLength="256" DataType="String" TitleResourceKey="ResourceKey_Utility">
			<DataBind>
				<Dependencies>
					<string>OldID</string>
				</Dependencies>
        <Columns>
          <Column Ident="ResourceKey" DataBindType="Value" />
        </Columns>
        <SQL>
					SELECT ResourceKey
					FROM dbo.NotificationSettingGroup
					WHERE ID = @OldID
        </SQL>
        <Parameters>
          <dsp:Parameter xsi:type="dsp:VariableParameter" Ident="OldID" DataType="String" />
        </Parameters>
      </DataBind>
    </Control>

		<Control xsi:type="TextBoxControl" Ident="SenderType" DataType="Number" TitleResourceKey="SenderType_Utility">
			<DataBind>
				<Dependencies>
					<string>OldID</string>
				</Dependencies>
        <Columns>
          <Column Ident="SenderType" DataBindType="Value" />
        </Columns>
        <SQL>
					SELECT SenderType
					FROM dbo.NotificationSettingGroup
					WHERE ID = @OldID
        </SQL>
        <Parameters>
          <dsp:Parameter xsi:type="dsp:VariableParameter" Ident="OldID" DataType="String" />
        </Parameters>
      </DataBind>
    </Control>

		<Control xsi:type="TextBoxControl" Ident="FolderGroupSegmentIdent" MaxLength="256" DataType="String" TitleResourceKey="FolderGroupSegmentIdent_Utility">
			<DataBind>
				<Dependencies>
					<string>OldID</string>
				</Dependencies>
        <Columns>
          <Column Ident="FolderGroupSegmentIdent" DataBindType="Value" />
        </Columns>
        <SQL>
					SELECT FolderGroupSegmentIdent
					FROM dbo.NotificationSettingGroup
					WHERE ID = @OldID
        </SQL>
        <Parameters>
          <dsp:Parameter xsi:type="dsp:VariableParameter" Ident="OldID" DataType="String" />
        </Parameters>
      </DataBind>
    </Control>


  </Controls>
	<Sections>
		<Section xsi:type="ContentSection" Ident="BasicInformation" TitleResourceKey="BasicInformation_Utility">
			<HTMLTemplate>
				<div class="row">
					<div class="col-md-3">
						<div class="form-group">
							<ControlLabel ControlID="OldID"/>
							<Control ID="OldID"/>
						</div>
					</div>
				</div>

				<legend class="font-size-sm font-weight-bold">dbo.NotificationSettingGroup</legend>

				<div class="row">
					<div class="col-md-3">
						<div class="form-group">
							<ControlLabel ControlID="NewID"/>
							<Control ID="NewID"/>
						</div>
					</div>
					<div class="col-md-3">
						<div class="form-group">
							<ControlLabel ControlID="ResourceKey"/>
							<Control ID="ResourceKey"/>
						</div>
					</div>
					<div class="col-md-3">
						<div class="form-group">
							<ControlLabel ControlID="SenderType"/>
							<Control ID="SenderType"/>
						</div>
					</div>
					<div class="col-md-3">
						<div class="form-group">
							<ControlLabel ControlID="FolderGroupSegmentIdent"/>
							<Control ID="FolderGroupSegmentIdent"/>
						</div>
					</div>
				</div>
				
			</HTMLTemplate>
		</Section>
	</Sections>
</Form>